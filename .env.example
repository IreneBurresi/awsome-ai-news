# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Logging Configuration
LOG_LEVEL=INFO

# Pipeline Configuration
DRY_RUN=false
MAX_RETRIES=3

# Cache Configuration
CACHE_DIR=./cache
CACHE_TTL_ARTICLES=864000  # 10 days in seconds
CACHE_TTL_NEWS=259200      # 3 days in seconds

# GitHub Configuration (for automated commits)
GIT_USER_NAME=awesome-ai-news-bot
GIT_USER_EMAIL=bot@example.com
